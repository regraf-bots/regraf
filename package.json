{
  "name": "regraf",
  "version": "1.0.4",
  "description": "Continuation of best modern Telegram Bot Framework",
  "license": "MIT",
  "author": "Ilya Petrov <me@redguy.ru>",
  "homepage": "https://github.com/RedGuys/regraf#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/RedGuys/regraf.git"
  },
  "bugs": {
    "url": "https://github.com/RedGuys/regraf/issues"
  },
  "main": "regraf.js",
  "files": [
    "bin/*",
    "core/**/*.js",
    "scenes/**/*.js",
    "typings/*.d.ts",
    "*.js"
  ],
